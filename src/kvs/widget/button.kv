# 带图标的按钮
<IconButton>
    id: icon_button
    width: dp(30)
    size_hint_x: None
    background_normal: app.background_normal
    background_down: app.background_down
    Image:
        source: root.icon_source
        pos: icon_button.x + dp(5), icon_button.y + dp(5)
        size: icon_button.width-dp(10), icon_button.height-dp(10)
