# 侧边栏模窗
<SidebarModalView>:
    id: sidebar_view
    overlay_color: self.overlay_color
    size_hint: 0.15, 1
    pos_hint: {'x':0}
    PartScrollView:
        PartBoxLayout:
            size_hint: 1,None
            orientation: 'vertical'
            spacing: dp(8)
            padding: dp(8)
            height: self.minimum_height

            BottomLineLabel:
                id: page_skinManage
                text: '皮肤管理'

# 主程序布局
<MainLayout>:
    ColorBoxLayout:
        id: main_content_layout
